{
  "name": "axiom-website",
  "description": "The website for Axiom Data Science",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "clean": "gatsby clean",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "date-fns": "^2.14.0",
    "gatsby": "^2.13.62",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-catch-links": "^2.1.3",
    "gatsby-plugin-netlify": "^2.1.4",
    "gatsby-plugin-netlify-cms": "^4.1.6",
    "gatsby-plugin-nprogress": "^2.1.2",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sass": "^2.1.8",
    "gatsby-plugin-sharp": "^2.2.11",
    "gatsby-plugin-sitemap": "^2.2.6",
    "gatsby-remark-copy-linked-files": "^2.1.5",
    "gatsby-remark-images": "^3.1.11",
    "gatsby-remark-prismjs": "^3.3.5",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-remark-responsive-iframe": "^2.2.4",
    "gatsby-remark-smartypants": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.9",
    "gatsby-transformer-remark": "^2.6.14",
    "gatsby-transformer-sharp": "^2.2.6",
    "gatsby-transformer-yaml": "^2.2.4",
    "lodash": "^4.17.15",
    "modern-normalize": "^0.5.0",
    "netlify-cms-app": "^2.9.7",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.1.0",
    "react-slick": "^0.26.1",
    "slate": "^0.47.4",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.0",
    "webpack": "^4.39.2",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/axiom-data-science/website"
  },
  "bugs": {
    "url": "https://github.com/axiom-data-science/website/issues"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "rules": {
      "consistent-return": 0,
      "max-len": 0,
      "no-console": 0,
      "react/prop-types": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/no-unescaped-entities": 0,
      "react/jsx-props-no-spreading": 0
    }
  }
}
